{
  "name": "vite-plugin-not-using",
  "version": "0.0.1-beta.0",
  "packageManager": "pnpm@7.9.0",
  "description": "> analyze project files and find out which fields are not used",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "zwkang",
    "email": "kang95630@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.coim/zwkang/vite-plugin-not-using.git"
  },
  "bugs": "https://github.coim/zwkang/vite-plugin-not-using/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vite": "^2.9.0 || ^3.0.0"
  },
  "scripts": {
    "dev": "tsup --watch",
    "lint": "eslint --cache .",
    "test": "vitest run",
    "build": "tsup",
    "start": "tsx src/index.ts",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prettier:all": "prettier -w ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.22.0",
    "eslint-config-alloy": "^4.6.2",
    "prettier": "^2.7.1",
    "tsup": "^6.2.2",
    "tsx": "^3.8.2",
    "typescript": "^4.7.4",
    "vitest": "^0.22.1",
    "zwkang-prettier-config": "^0.0.3",
    "@types/node": "^20.4.0",
    "@types/fs-extra": "^11.0.1"
  },
  "license": "MIT",
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "fs-extra": "^11.1.1",
    "picocolors": "^1.0.0"
  }
}
